<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="op27no2.fitness.thirtymm.ui.lifting.LiftFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_lift" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="op27no2.fitness.thirtymm.ui.nutrition.NutritionFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_nutrition" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="op27no2.fitness.thirtymm.ui.history.HistoryFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_history" />

    <fragment
        android:id="@+id/navigation_run"
        android:name="op27no2.fitness.thirtymm.ui.run.RunFragment"
        android:label="@string/title_run"
        tools:layout="@layout/fragment_run" />

    <fragment
        android:id="@+id/navigation_sleep"
        android:name="op27no2.fitness.thirtymm.ui.sleep.SleepFragment"
        android:label="@string/title_sleep"
        tools:layout="@layout/fragment_sleep" />

    <fragment
        android:id="@+id/navigation_volume"
        android:name="op27no2.fitness.thirtymm.ui.volume.VolumeFragment"
        android:label="@string/title_volume"
        tools:layout="@layout/fragment_volume" />
</navigation>